/* pages/play/play.wxss */
page,.page-contair{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color:darkslateblue;
}
button::after{
  border:0;
}
.btn-play,.btn-pause{
  width:100px;
  height:100px;
  background:none;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat; 
  background-size:100% 100%;
  -moz-background-size:100% 100%;
  border:0;
}
.btn-play-press{
  background-image:url(data:image/png;base64,);
}
.btn-pause{
  background-image: url(data:image/png;base64,);
}
.btn-pause-press{
  background-image: url(data:image/png;base64,);
}